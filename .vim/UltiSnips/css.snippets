snippet "@font-face " "desc" r
@font-face {
    font-family: '${1:robotolight}';
    src: url('${2:roboto-light-webfont}.eot');
    src: local('☺'),
         url('$2.eot?#iefix') format('embedded-opentype'),
         url('$2.woff2') format('woff2'),
         url('$2.woff') format('woff'),
         url('$2.ttf') format('truetype'),
         url('$2.svg#$1') format('svg');
    font-weight: ${3:normal};
    font-style: ${4:normal};
}
endsnippet
